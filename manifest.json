{
  "manifest_version": 3,
  "name": "My Todo Lists",
  "version": "1.0",
  "description": "Organize suas tarefas por dia, semana, mÃªs e ano",
  "permissions": [
    "storage",
    "sidePanel",
    "activeTab"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "My Todo Lists"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/notes_16x16.png",
    "32": "icons/notes_32x32.png",
    "48": "icons/notes_48x48.png",
    "128": "icons/notes_128x128.png"
  }
}